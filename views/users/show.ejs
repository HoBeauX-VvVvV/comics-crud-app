<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= pageOwner.username %>'s Collection</title>
</head>
<body>
  <%- include('../partials/_navbar.ejs') %>
  <h1>Welcome to <%= pageOwner.username %>'s collection</h1>
  <p>They have:</p>
  <ul>
    <% pageOwner.comicsCollection.forEach((comic)=>{ %>
        <li><a href="/users/<%= user._id %>/comics/<%= comic._id %>">
            <%= comic.title %> </a></li>
    <% }) %>
  </ul>
</body>
</html>